#==================================
# Classes and Datatype Properties
#==================================

# Pessoa
:Pessoa
rdfs:label "Pessoa"@pt;
rdfs:comment "Criatura humana ou indivíduo humano enquanto sujeito de direitos e deveres."@pt;
owl:equivalentClass schema:Person.

:nomePessoa
a owl:FunctionalProperty;
rdfs:label "nome da pessoa"@pt;
rdfs:comment "Nome completo e por extenso da pessoa"@pt;
owl:equivalentProperty foaf:name,
                       dc:name.

:funcaoPessoa
a owl:FunctionalProperty;
rdfs:label "função"@pt;
rdfs:comment "Função exercida por uma pessoa."@pt;
owl:equivalentProperty bibframe:role.

:emailInstitucionalPessoa
a owl:FunctionalProperty;
rdfs:label "e-mail institucional"@pt;
rdfs:comment "Trata-se do e-mail Institucional de uma pessoa."@pt;
owl:equivalentProperty foaf:mbox,
                       pext:hasEMail.

:emailAlternativoPessoa
a owl:FunctionalProperty;
rdfs:label "e-mail alternativo"@pt;
rdfs:comment "Trata-se do e-mail alternativo de uma pessoa."@pt;
owl:equivalentProperty km4c:alternativeEmail.

# Acervo
:Acervo
rdfs:label "Acervo"@pt;
rdfs:comment "Local que contém todos os documentos depositados em suas bibliotecas e que estão à disposição do público para estudo e pesquisa, na dependência específica de reprodução."@pt;
owl:equivalentClass fabio:WorkCollection,
                    fabio:DocumentRepository.
		
# Cadastrador
:Cadastrador 
rdfs:label "Cadastrador"@pt;
rdfs:comment "Trata-se de quem cadastra as teses e as dissertações em um Acervo."@pt;
owl:equivalentClass caresses:User.

# Discente
:Discente
rdfs:label "Discente"@pt;
rdfs:comment "Na Educação Superior o verbete aluno refere-se ao estudante. Trata-se do enfoque no processo de aprendizagem, dos saberes discentes e dos diferentes tipos de estudantes (considerados em suas distinções pessoais e nas circunstâncias da academia)."@pt;
owl:equivalentClass sio:SIO_000405,
                    seo:Student.
            
:matriculaDiscente
a owl:FunctionalProperty;
rdfs:label "matrícula do discente"@pt;
rdfs:comment "A matrícula de um discente é o seu identificador único."@pt;
owl:equivalentProperty dc:identifier.

:telefoneDiscente
a owl:FunctionalProperty;
rdfs:label "telefone do discente"@pt;
rdfs:comment "Diz respeito ao número de telefone do discente."@pt;
owl:equivalentProperty foaf:phone.										

# Discente Egresso
:DiscenteEgresso
rdfs:label "Discente Egresso"@pt ;
rdfs:comment "."@pt;
owl:equivalentClass sio:SIO_000399.

# Membro
:Membro
rdfs:label "Membro"@pt;
rdfs:comment "Trata-se de integrante da comissão examinadora de defesa de qualificação, dissertação ou tese interno ou externo ao programa de pós-graduação e à Instituição que participa."@pt;
owl:equivalentClass foaf:Group,
                    scoro:EducationalRole,
                    vivo:MemberRole.

:modalidadeParticipacaoMembro
a owl:FunctionalProperty;
rdfs:label "modalidade de participação"@pt;
rdfs:comment "Trata-se da modalidade de participação da banca de avaliação por parte do membro: presencial ou online."@pt;
owl:equivalentProperty pproc:criterionEvaluationMode.

# Digitalizador
:Digitalizador
rdfs:label "Digitalizador"@pt;
rdfs:comment "Trata-se de uma pessoa institucionalizada que digitaliza as teses e as dissertações."@pt.

# Bolsa de Estudo
:BolsaEstudo
rdfs:label "Bolsa de Estudo"@pt;
rdfs:comment "Trata-se de um patrocínio do governo."@pt;
owl:equivalentClass frapo:scholarship,
                    frapo:Studentship.

# Bolsista
:Bolsista
rdfs:label "Bolsista"@pt;
rdfs:comment "É todo aluno que recebe uma Bolsa de Estudos."@pt;
owl:equivalentClass scoro:FinancialRole,
                    sio:SIO_000406.

# Não Bolsista
:NaoBolsista
rdfs:label "NaoBolsista"@pt;
rdfs:comment "Aluno que faz parte de uma pesquisa acadêmica por espontânea vontade e sem retribuição financeira."@pt.

# Orientador
:Orientador
rdfs:label "Orientador"@pt;
rdfs:comment "É membro e presidente da Banca Examinadora. Caracteriza-se pelo processo de acompanhamento, tutoria, direcionamento e apoio ao estudante orientando em uma série de aspectos pesquisados, que são decisivos na formação dos futuros orientadores e pesquisadores."@pt;
owl:equivalentClass mrel:ths,
                    sio:SIO_000878, # (Orientador)
                    mrel:Thesisadvisor.

# Coorientador
:Coorientador
rdfs:label "Coorientador"@pt;
rdfs:comment "É membro da Banca Examinadora (BRASIL, 2022b) que é caracterizado por acompanhar o processo de tutoria, direcionar e apoiar ao aluno orientando em uma série de aspectos pesquisados, que são decisivos na formação dos futuros orientadores e pesquisadores."@pt;
owl:equivalentClass mrel:ths,
                    sio:SIO_000878, #(Orientador)
                    mrel:Thesisadvisor.

# Patrocinador
:Patrocinador
rdfs:label "Patrocinador"@pt;
rdfs:comment "Trata-se do patrocinador que disponibiliza Bolsa de Estudo."@pt;
owl:equivalentClass frapo:FinancialEntity,
                    frapo:FundingProgramme.

:nomePatrocinador
a owl:FunctionalProperty;
rdfs:label "nome do patrocinador"@pt;
rdfs:comment "Trata-se do nome do Patrocinador."@pt.

# Contrato
:Contrato
rdfs:label "Contrato"@pt;
rdfs:comment "Diz respeito ao contrato de bolsa de estudos que é disponibilizado por um patrocinador (agência de fomento)."@pt.

:inicioContrato
a owl:FunctionalProperty;
rdfs:label "início"@pt;
rdfs:comment "Diz respeito a data de início referente um contrato que um bolsista recebe por bolsa de estudos."@pt.

:fimContrato
a owl:FunctionalProperty;
rdfs:label "fim"@pt;
rdfs:comment "Diz respeito a data fim em um contrato que um bolsista recebe por bolsa de estudos."@pt.

# Defesa
:Defesa
rdfs:label "Defesa"@pt ;
rdfs:comment "."@pt;
owl:equivalentClass schema:EducationEvent,
                    sro:Evaluation,
                    sro:Discussion,
                    sro:Argument,
                    sro:Claim.

:dataHoraAvaliacaoDefesa
a owl:FunctionalProperty;
rdfs:label "data e hora de avaliação"@pt;
rdfs:comment "Diz respeito a data e a hora de avaliação de uma defesa."@pt;
owl:equivalentProperty fabio:hasDecisionDate,
                       dcat:record,
                       ptop:registeredIn.

# Cadastro
:Cadastro
rdfs:label "Cadastro"@pt;
rdfs:comment "."@pt;
owl:equivalentClass fabio:ArchivalRecordSet.

:identificadorRegistroCadastro
a owl:FunctionalProperty;
rdfs:label "identificador do registro"@pt;
rdfs:comment "Diz respeito ao identificador único de um registro quanto ao Cadastro referente ao Trabalho de Conclusão de Curso que ocorre no acervo."@pt;
owl:equivalentProperty frapo:identifier.

:dataHoraRegistroCadastro
a owl:FunctionalProperty;
rdfs:label "data/hora do registro"@pt;
rdfs:comment "Diz respeito a data/hora de um registro quanto ao Cadastro referente ao Trabalho de Conclusão de Curso que ocorre no acervo."@pt;
owl:equivalentProperty fabio:hasDateCollected,
                       bibframe:date,
                       dc:date,
                       dc:dateSubmitted.

# Trabalho de Conclusão de Curso
:TrabalhoConclusaoCurso
rdfs:label "Trabalho de Conclusão de Curso"@pt;
rdfs:comment "Deve ser um trabalho acadêmico de finalização do curso, sendo necessariamente, pelo menos, na modalidade de uma monografia, ou outros trabalhos técnicos associados quando previstos. São trabalhos que retratam o resultado de estudo ou pesquisa sobre um tema, sejam eles exigidos por disciplina, módulo, estudo independente, curso e programa."@pt;
owl:equivalentClass schema:Thesis,
                    fabio:ScholarlyWork,
                    frbrer:C1004,
                    dbpedia:Trabalho_de_conclusão_de_curso.

# Banca
:Banca
rdfs:label "Banca"@pt;
rdfs:comment "Trata-se da composição de membros (no mínimo três titulares e um suplente) para avaliar a defesa de um estudante de pós-graduação em uma Instituição de Ensino Superior."@pt;
owl:equivalentClass fabio:ExaminationPaper,
                    scoro:ContributionSituation. #(scoro)

# Examinador
:Examinador
rdfs:label "Examinador"@pt;
rdfs:comment "Trata-se de integrante da comissão examinadora de defesa de qualificação, dissertação ou tese. É o membro que recebe o convite para participar de uma banca de defesa de qualificação, dissertação ou tese."@pt;
owl:equivalentClass sur:Participant,
                    cwmo:Evaluator.

# Examinador Interno
:ExaminadorInterno
rdfs:label "Examinador Interno"@pt ;
rdfs:comment "."@pt;
owl:equivalentClass sur:Participant,
                    cwmo:Evaluator.

# Examinador Externo
:ExaminadorExterno
rdfs:label "Examinador Externo"@pt ;
rdfs:comment "."@pt;
owl:equivalentClass sur:Participant,
                    cwmo:Evaluator.

# Documento de Aprovação
:DocumentoDeAprovacao
rdfs:label "Documento de Aprovação"@pt;
rdfs:comment " "@pt;
owl:equivalentClass schema:EducationalOccupationalCredential,
                    fabio:Diary.

# Pessoa Institucionalizada
:PessoaInstitucionalizada
rdfs:label "Pessoa Institucionalizada"@pt;
rdfs:comment "Refere-se a um indivíduo humano associado a uma Instituição de Ensino Superior."@pt;
owl:equivalentClass scoro:EducationalRole,
                    whois:academicCareer. #(whois)

:orcidPessoaInstitucionalizada
a owl:FunctionalProperty;
rdfs:label "orcid"@pt;
rdfs:comment "Diz respeito ao orcid de uma pessoa associada a uma Instituição."@pt;
owl:equivalentProperty scoro:hasORCID.

:lattesPessoaInstitucionalizada
a owl:FunctionalProperty;
rdfs:label "lattes"@pt;
rdfs:comment "Diz respeito ao lattes de uma pessoa associada a uma Instituição."@pt;
owl:equivalentProperty frapo:hasCurriculumVitae.

# Programa de Pós-Graduação
:ProgramaPosGraduacao
rdfs:label "Programa de Pós-Graduação"@pt;
rdfs:comment "Trata-se de uma programação de curso de pós-graduação que são criados quando constatados da sua viabilidade, relevância e número adequado de professores pesquisadores com titulação e produção científica suficientes para dar sustentação à criação de curso de mestrado e/ou doutorado em determinada área de conhecimento, bem como a sua adequação aos critérios da Coordenação de Aperfeiçoamento de Pessoal do Ensino Superior - Capes para apresentação e propostas de cursos novos. Programa de Pós-Graduação pode ser gerido por uma única Instituição de Ensino Superior ou por Rede de Ensino. Assim, compreende programa de mestrado, doutorado, cursos de especialização, aperfeiçoamento, dentre outros, e são abertos a candidatos diplomados em cursos de graduação e que atendam os requisitos das instituições de ensino. Compreende programas de mestrado e doutorado."@pt;
owl:equivalentClass fabio:AcademicProceedings,
                    schema:EducationalOccupationalProgram,
                    teach:StudyProgram, #(teach)
                    xapi:Grouping.

:nomeProgramaPosGraduacao
a owl:FunctionalProperty;
rdfs:label "nome"@pt;
rdfs:comment "Trata-se de uma programação de curso de pós-graduação que são criados quando constatados da sua viabilidade, relevância e número adequado de professores pesquisadores com titulação e produção científica suficientes para dar sustentação à criação de curso de mestrado e/ou doutorado em determinada área de conhecimento, bem como a sua adequação aos critérios da Coordenação de Aperfeiçoamento de Pessoal do Ensino Superior - Capes para apresentação e propostas de cursos novos."@pt;
owl:equivalentProperty xapi:hasName.

# Tese
:Tese
rdfs:label "Tese"@pt;
rdfs:comment "Colaboração acadêmica original para o conhecimento e apresentada para obter o grau acadêmico de doutor e dos títulos universitários livre-docente e professor titular. Constitui o trabalho de pesquisa original e individual que forneça contribuição importante para a área de conhecimento na qual está inserida."@pt;
owl:equivalentClass fabio:DoctoralThesis,
                    fabio:Thesis,
                    fabio:Proposition.

# Dissertação
:Dissertacao
rdfs:label "Dissertação"@pt;
rdfs:comment "owl:class Curso Dissertação é o resultado de um trabalho de pesquisa desenvolvido, documentado e defendido publicamente em cursos de pós-graduação de mestrado, destinado a obtenção do grau acadêmico de mestre."@pt;
owl:equivalentClass bibframe:Dissertation,
                    fabio:MastersThesis,
                    sio:SIO_000167. #(dissertação)

# Documento
:Documento
rdfs:label "Documento"@pt;
rdfs:comment "Trata-se de um documento digital ou impresso para o contexto aplicado."@pt;
owl:equivalentClass foaf:Document,
                    dbpedia:Documento.

:identificacaoUnicaDocumento
a owl:FunctionalProperty;
rdfs:label "identificação única"@pt;
rdfs:comment "Trata-se de uma identificação inequívoca de um documento."@pt;
owl:equivalentProperty fabio:identifier.

:tituloDocumento
a owl:FunctionalProperty;
rdfs:label "título"@pt;
rdfs:comment "Trata-se do título de um documento."@pt;
owl:equivalentProperty bibframe:title,
                       fabio:title.

:resumoDocumento
a owl:FunctionalProperty;
rdfs:label "resumo"@pt;
rdfs:comment "Trata-se do resumo de um documento."@pt;
owl:equivalentProperty bibframe:summary,
                       fabio:abstract.

:semestreLetivoDocumento
a owl:FunctionalProperty;
rdfs:label "semestre letivo"@pt;
rdfs:comment "Trata-se do semestre letivo de um documento."@pt;
owl:equivalentProperty ov:semester.

:idiomaDocumento
a owl:FunctionalProperty;
rdfs:label "idioma"@pt;
rdfs:comment "Trata-se do idioma de um documento."@pt;
owl:equivalentProperty bibframe:language.

:numeroPaginasDocumento
a owl:FunctionalProperty;
rdfs:label "número de páginas"@pt;
rdfs:comment "Trata-se do número de páginas de um documento."@pt;
owl:equivalentProperty bibframe:count,
                       fabio:hasPageCount.

:uriDisseminacaoDocumento
a owl:FunctionalProperty;
rdfs:label "uri disseminação"@pt;
rdfs:comment "Trata-se da uri a ser disseminada de um documento."@pt;
owl:equivalentProperty meb:uri. #(meb)

# Documento Impresso
:DocumentoImpresso
rdfs:label "Documento Impresso"@pt;
rdfs:comment "São artefatos digitais. Qualquer escrito ou impresso que fornece uma informação ou prova, usado para esclarecimento de algo. Neste caso, trata-se de Trabalhos de Conclusão de Curso como Teses e Dissertações."@pt.

# Documento Digital
:DocumentoDigital
rdfs:label "Documento Digital"@pt;
rdfs:comment "Entende-se por documento digital a conversão da fiel imagem de um documento para o código digital.Trata-se de um documento que pode ser digitalizado (o ato ou o efeito da conversão de um documento que contém informação ou dados analógicos para um formato digital) ou nato digital (documento que já nasce digitalmente)."@pt;
owl:equivalentClass fabio:DigitalItem.

:nomePDF
a owl:FunctionalProperty;
rdfs:label "nome do PDF"@pt;
rdfs:comment "Diz respeito ao nome do PDF."@pt;
owl:equivalentProperty foaf:name.

:tipoPDF
a owl:FunctionalProperty;
rdfs:label "tipo do PDF"@pt;
rdfs:comment "Diz respeito ao tipo do PDF (Os tipos de PDF são: PDF/A-1a, PDF/A-1b, PDF/A-21, PDF/A-2b, PDF/A-2u, PDF/A-3a, PDF/A-3b, PDF/A-3u, PDF/A-4f, PDF/A-4e)."@pt;
owl:equivalentProperty bibframe:digitalCharacteristic.

:tamanhoKilobites
a owl:FunctionalProperty;
rdfs:label "tamanho em kilobites"@pt;
rdfs:comment "Diz respeito ao tamanho em kilobites."@pt;
owl:equivalentProperty bibframe:FileSize.

:descricaoAnexos
a owl:FunctionalProperty;
rdfs:label "descrição dos anexos"@pt;
rdfs:comment "Diz respeito a descrição dos anexos."@pt;
owl:equivalentProperty bibframe:supplementaryContent.

# Nato Digital
:NatoDigital
rdfs:label "Nato Digital"@pt;
rdfs:comment "É um documento que já nasce em formato digital. Elaborado originalmente em meio eletrônico como documento, contrato, diploma, publicação, material, conteúdo, etc. Trata-se de uma modalidade digital."@pt.

# Digitalizado
:Digitalizado
rdfs:label "Digitalizado"@pt;
rdfs:comment "É a representação digital do processo de digitalização do documento físico e seus metadados, isto é, trata-se de um documento digital no estado digitalizado. Um documento deste tipo tem o mesmo efeito jurídico conferido aos documentos microfilmados, nos termos da Lei nº 5.433, de 8 de maio de 1968, e de regulamentação posterior. São documentos no formato digital e que foram digitalizados (eram físicos mas passaram a ser documentos digitais)."@pt.

:dataDigitalizacaoDigitalizado
a owl:FunctionalProperty;
rdfs:label "data da digitalização"@pt;
rdfs:comment "Diz respeito a data de digitalização de um Documento Digital Digitalizado."@pt;
owl:equivalentProperty fabio:hasAccessDate,
                       bibframe:date,
                       dc:date,
                       dcndl:dateDigitized.

:modoOcrImagemDigitalizado
a owl:FunctionalProperty;
rdfs:label "modo OCR imagem"@pt;
rdfs:comment "Diz respeito ao modo OCR imagem."@pt;
owl:equivalentProperty bibframe:media.

# Local
:Local
rdfs:label "Local"@pt;
rdfs:comment "Trata-se do lugar físico ou virtual de registro do Trabalho de Conclusão de Curso. Quanto ao local físico, refere-se ao nome da cidade de apresentação do trabalho acadêmico. Neste caso, refere-se ao Trabalho de Conclusão de Curso de pós-graduação stricto sensu (dissertação ou tese). Quando se refere ao local virtual, diz respeito ao nome do caminho ou diretório de um computador ou no âmbito Web."@pt;
owl:equivalentClass bibframe:Place,
                    schema:Place,
                    dc:Location.

# Local Físico
:LocalFisico
rdfs:label "Local Físico"@pt;
rdfs:comment "Trata-se do local físico (concreto) dos documentos acadêmicos e institucionais."@pt;
owl:equivalentClass bibframe:Place,
                    schema:Place,
                    dc:Location.

# rdfs:comment "Bases promissoras para reconciliação de indivíduos e enriquecimento semântico com owl:sameAs = wde, gn" .

:cidade
a owl:FunctionalProperty;
rdfs:label "cidade"@pt;
rdfs:comment "Diz respeito ao nome da cidade e que corresponde ao local físico."@pt;
owl:equivalentProperty wdo:Q82799, #(name)
                       schema:name,
                       gn:name.

:estado
a owl:FunctionalProperty;
rdfs:label "estado"@pt;
rdfs:comment "Diz respeito ao nome do estado e que corresponde ao local físico."@pt;
owl:equivalentProperty wdo:Q82799, #(name)
                       schema:name,
                       gn:name.

:pais
a owl:FunctionalProperty;
rdfs:label "país"@pt;
rdfs:comment "Diz respeito ao nome do país e que corresponde ao local físico."@pt;
owl:equivalentProperty wdo:Q82799, #(name)
                       schema:name,
                       gn:name.

# Local Virtual
:LocalVirtual
rdfs:label "Local Virtual"@pt;
rdfs:comment "Trata-se do local (caminho) virtual dos documentos acadêmicos e institucionais . Refere-se ao caminho do computador ou na internet que pode tratar um lugar virtual como se fosse um lugar físico, mas o local virtual é, na realidade, uma parte da memória do computador usada como se fosse um local físico."@pt.

:descricaoCaminhoLocalVirtual
a owl:FunctionalProperty;
rdfs:label "descrição do caminho"@pt;
